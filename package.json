{
  "name": "money-tracker",
  "private": true,
  "version": "0.0.0",
  "description": "MoneyTracker is a personal finance management application that helps users track their income and expenses for an overview of their finances.",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=./apps/*",
    "lint": "turbo run lint --",
    "lint:css": "turbo run lint:css --",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "author": "Tobias Peters",
  "repository": {
    "url": "https://github.com/pt232/money-tracker",
    "directory": "server",
    "type": "git"
  },
  "homepage": "https://github.com/pt232/money-tracker#readme",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "turbo": "^1.7.0"
  }
}
