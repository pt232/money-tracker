{
  "name": "money-tracker-api",
  "private": true,
  "version": "0.0.0",
  "description": "This server powers the MoneyTracker application, enabling the management and tracking of financial transactions and data.",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "npm run build:clean && npm-run-all --parallel build:watch dev:watch",
    "dev:watch": "nodemon --quiet dist/server.js",
    "build": "npm run build:clean && tsc --project tsconfig.build.json",
    "build:clean": "rimraf dist",
    "build:watch": "tsc --project tsconfig.build.json --watch",
    "start": "node dist/server.js"
  },
  "author": "Tobias Peters",
  "repository": {
    "url": "https://github.com/pt232/money-tracker",
    "directory": "server",
    "type": "git"
  },
  "homepage": "https://github.com/pt232/money-tracker#readme",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "concurrently": "^7.6.0",
    "eslint-config-custom": "*",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsconfig": "*",
    "typescript": "^4.9.4"
  }
}
